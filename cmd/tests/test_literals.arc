namespace main

func main() int32 {
    // Boolean literals
    let flag: bool = true
    let enabled: bool = false
    
    // Null pointer
    let ptr: *int32 = null
    
    // Character literals
    let ch: char = 'a'
    let digit: char = '5'
    
    // Character escapes
    let newline: char = '\n'
    let tab: char = '\t'
    let backslash: char = '\\'
    let quote: char = '\''
    let null_char: char = '\0'
    
    // String literals
    let msg: string = "Hello, World!"
    let empty: string = ""
    
    // String escapes
    let multiline: string = "Hello\nWorld"
    let path: string = "C:\\Users\\file"
    let quoted: string = "He said \"hello\""
    let tabbed: string = "Column1\tColumn2"
    
    return 0
}