define external i32 @add(i32 %a, i32 %b) {
entry:
  %0 = add i32 %a, %b
  ret i32 %0
}

define external i32 @main() {
entry:
  %x = alloca i32
  store i32 100, ptr<i32> %x
  %y = alloca i32
  store i32 42, ptr<i32> %y
  %1 = load i32, ptr<i32> %x
  %2 = load i32, ptr<i32> %y
  %3 = call i32 @add(i32 %1, i32 %2)
  ret i32 %3
}
